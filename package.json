    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.12.0",
    "ember-test-utils": "^8.1.1",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
